<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>dst-reminder</application>
  <version>29</version>
  
  <ssl-enabled>true</ssl-enabled>
  <sessions-enabled>false</sessions-enabled>
  
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties" />
    <property name="org.apache.commons.logging.Log" value="org.apache.commons.logging.impl.Jdk14Logger" />
    <property name="javax.xml.stream.XMLInputFactory" value="com.ctc.wstx.stax.WstxInputFactory" />
    <property name="javax.xml.stream.XMLOutputFactory" value="com.ctc.wstx.stax.WstxOutputFactory" />
    <property name="javax.xml.parsers.DocumentBuilderFactory" value="org.apache.xerces.jaxp.DocumentBuilderFactoryImpl" />
  </system-properties>
  
</appengine-web-app>